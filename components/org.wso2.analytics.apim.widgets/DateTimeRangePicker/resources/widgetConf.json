{
  "id": "DateRangePicker",
  "name": "DATE TIME RANGE",
  "thumbnailURL": "",
  "configs": {
    "pubsub": {
      "types": ["publisher"],
      "publisherWidgetOutputs": ["granularity", "from", "to"]
    },
    "options": [
      {
        "id": "header",
        "title": "Header",
        "type": {
          "name": "BOOLEAN",
          "possibleValues": [true, false]
        },
        "defaultValue": true
      },
      {
        "id": "defaultValue",
        "title": "Default Range",
        "type": {
          "name": "ENUM",
          "possibleValues": [
            "Last Hour",
            "Last Day",
            "Last 7 Days",
            "Last Month",
            "Last 3 Months",
            "Last 6 Months",
            "Last Year"
          ]
        },
        "defaultValue": "Last 3 Months"
      },
      {
        "id": "availableGranularities",
        "title": "Available Granularities",
        "type": {
          "name": "ENUM",
          "possibleValues": [
            "From Second",
            "From Minute",
            "From Hour",
            "From Day",
            "From Month",
            "From Year"
          ]
        },
        "defaultValue": "From Second"
      },
      {
        "id": "autoSyncInterval",
        "title": "Auto-Sync Interval (Seconds)",
        "type": {
          "name": "TEXT"
        },
        "defaultValue": "30"
      },
      {
        "id": "headerTitle",
        "title": "Widget Title",
        "type": {
          "name": "TEXT"
        },
        "defaultValue": "Date Time Range"
      }
    ]
  }
}
