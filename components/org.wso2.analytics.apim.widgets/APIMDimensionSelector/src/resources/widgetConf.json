{
  "name" : "APIM Dimension Selector",
  "id" : "APIMDimensionSelector",
  "thumbnailURL" : "",
  "configs" : {
    "pubsub" : {
      "types": ["publisher"],
      "publisherWidgetOutputs": ["dimension", "selection"]
    },
    "options": [
      {
        "id" : "header",
        "title" : "Header",
        "type" : {
          "name" : "BOOLEAN",
          "possibleValues" : [
            true,
            false
          ]
        },
        "defaultValue": false
      },
      {
        "id" : "selectMultiple",
        "title" : "Select Multiple",
        "type" : {
          "name" : "BOOLEAN",
          "possibleValues" : [
            true,
            false
          ]
        },
        "defaultValue": true
      },
      {
        "id" : "dimensions",
        "title" : "Dimension(s)",
        "type" : {
          "name" : "ENUM",
          "multiple" : true,
          "possibleValues" : [
            "API",
            "API Provider"
          ]
        },
        "defaultValue": ["API", "API Provider"]
      },
      {
        "id" : "defaultDimension",
        "title" : "Default Dimension",
        "type" : {
          "name" : "ENUM",
          "possibleValues" : [
            "API",
            "API Provider"
          ]
        },
        "defaultValue": "API"
      }
    ]
  }
}
